"""nohands -- personal financial management"""

from nohands.accountant import Accountant

__version__ = "0.2.0"
__author__ = "Jesse Butcher <boweeb@gmail.com>"
__license__ = "MIT"


def main():
    a = Accountant()

    a.report_earners()
    a.report_expenses()
    a.report_giving()
    a.report_savings()
    a.report_deposits()
    a.report_ffg()

if __name__ == '__main__':
    main()


